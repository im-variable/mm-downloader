{
  "expo": {
    "name": "trackdown",
    "slug": "mm-downloader",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/getyourtrack.png",
    "splash": {
      "image": "./assets/getyourtrack-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#e09794"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/getyourtrack.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": ["READ_EXTERNAL_STORAGE", "WRITE_EXTERNAL_STORAGE"],
      "package": "com.sandeepnegi.mmdownloader"
    },
    "web": {
      "favicon": "./assets/getyourtrack.png"
    },
    "extra": {
      "eas": {
        "projectId": "f45e302d-94e3-430b-9712-b6372a1ded16"
      }
    },
    "owner": "sandeepnegi",
    "plugins": [
      [
        "expo-media-library",
        {
          "isAccessMediaLocationEnabled": true
        }
      ],
      "expo-build-properties"
    ]
  },
  "react-native-google-mobile-ads": {
    "android_app_id": "ca-app-pub-7734815935340334~2084243836"
  }
}
